{
  "name": "problem-solving-automation",
  "version": "1.0.0",
  "description": "Automated problem solving using LLM and GitHub integration",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.js",
    "dev": "bun --watch src/index.js",
    "decompose": "bun run src/index.js decompose",
    "solve": "bun run src/index.js solve",
    "compose": "bun run src/index.js compose",
    "cleanup": "bun run src/index.js cleanup",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:all": "bun run tests/runTests.js",
    "test:integration": "bun test tests/githubIntegration.test.js",
    "test:unit": "bun test tests/orchestrator.test.js tests/repositoryManager.test.js tests/githubClient.test.js",
    "test:workflow": "bun run src/github/workflowTest.js",
    "lint": "eslint src/**/*.js tests/**/*.js",
    "lint:fix": "eslint src/**/*.js tests/**/*.js --fix"
  },
  "keywords": [
    "problem-solving",
    "automation",
    "llm",
    "github",
    "ai"
  ],
  "author": "",
      "license": "Unlicense",
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "commander": "^14.0.0",
    "dotenv": "^17.0.1",
    "langchain": "^0.3.29",
    "@langchain/openai": "^0.5.18",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
} 